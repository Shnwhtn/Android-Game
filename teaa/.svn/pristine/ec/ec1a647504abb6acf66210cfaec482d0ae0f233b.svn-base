<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mobile.labs.acw.puzzleInfo">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/puzzlewins"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:textStyle="bold"
            tools:text="@string/PuzzleLayout"
            android:layout_below="@+id/puzzlepicturesetvar"
            android:layout_alignLeft="@+id/puzzlelayout"
            android:layout_alignStart="@+id/puzzlelayout" />

        <EditText
            android:id="@+id/puzzlewinvar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:clickable="false"
            android:editable="false"
            android:ems="7"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:gravity="center"
            android:linksClickable="false"
            android:text="derp"
            android:layout_below="@+id/puzzlewins"
            android:layout_alignLeft="@+id/puzzlesizevar"
            android:layout_alignStart="@+id/puzzlesizevar" />

        <EditText
            android:id="@+id/puzzlenametitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:textStyle="bold"
            tools:text="@string/PuzzleNumber"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <EditText
            android:id="@+id/puzzlepicturesetvar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:ems="7"
            android:gravity="center"
            android:inputType="textPersonName"
            android:text="puzzleset"
            android:textAlignment="center"
            android:layout_below="@+id/puzzlelayout"
            android:layout_centerHorizontal="true" />

        <EditText
            android:id="@+id/puzzlesizevar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:background="@null"
            android:ems="7"
            android:gravity="center"
            android:inputType="textPersonName"
            android:text="puzzlesize"
            android:textAlignment="center"
            android:layout_alignTop="@+id/puzzlesize"
            android:layout_alignLeft="@+id/puzzlenamevar"
            android:layout_alignStart="@+id/puzzlenamevar" />

        <EditText
            android:id="@+id/puzzlenamevar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:ems="7"
            android:gravity="center"
            android:inputType="textPersonName"
            android:text="Name"
            android:textAlignment="center"
            android:layout_below="@+id/puzzlenametitle"
            android:layout_alignLeft="@+id/puzzlepicturesetvar"
            android:layout_alignStart="@+id/puzzlepicturesetvar" />

        <EditText
            android:id="@+id/puzzlesize"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:textStyle="bold"
            android:visibility="visible"
            tools:text="@string/PuzzleSize"
            android:layout_below="@+id/puzzlenamevar"
            android:layout_alignLeft="@+id/puzzlenametitle"
            android:layout_alignStart="@+id/puzzlenametitle" />

        <EditText
            android:id="@+id/puzzlelayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="19dp"
            android:background="@null"
            android:ems="10"
            android:gravity="center"
            android:textStyle="bold"
            tools:text="@string/PuzzleLayout"
            android:layout_below="@+id/puzzlesize"
            android:layout_alignLeft="@+id/puzzlesize"
            android:layout_alignStart="@+id/puzzlesize" />

        <Button
            android:id="@+id/downloadplay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:onClick="downloadorplay"
            android:text="Button"
            android:visibility="invisible"
            android:layout_below="@+id/puzzlewinvar"
            android:layout_centerHorizontal="true" />/>

    </RelativeLayout>

</FrameLayout>
